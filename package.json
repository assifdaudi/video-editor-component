{
  "name": "video-editor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build video-editor-lib",
    "build:lib:watch": "ng build video-editor-lib --watch --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:lib": "ng lint video-editor-lib",
    "lint:fix": "ng lint --fix",
    "lint:server": "cd server && npm run lint",
    "lint:server:fix": "cd server && npm run lint:fix",
    "lint:all": "npm run lint && npm run lint:lib && npm run lint:server",
    "dev:server": "cd server && npm run dev",
    "dev:client": "ng serve --port 4200",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "dashjs": "^5.1.0",
    "rxjs": "~7.8.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.0.1",
    "@angular-eslint/eslint-plugin": "^21.0.1",
    "@angular-eslint/eslint-plugin-template": "^21.0.1",
    "@angular-eslint/schematics": "^21.0.1",
    "@angular-eslint/template-parser": "^21.0.1",
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@eslint/js": "^9.39.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.8"
  }
}
